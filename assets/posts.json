layout: null
---
[
{% for post in site.posts limit:7 %}
  {
    "title": "{{ post.title | escape }}",
    "url": "{{ post.url | relative_url }}",
    "category": "{{ post.categories | first }}",
    "excerpt": "{{ post.excerpt | strip_html | truncate: 120 }}"
  }{% unless forloop.last %},{% endunless %}
{% endfor %}
]
